{
  "proto_path": "example/guber/proto",
  "lib_path": "example/guber/lib",
  "compile_proto": {
    "clean_services": true,
    "overwrite": true,
    "zipcompress": false,
    "outdir": "example/guber/dist"
  },
  "service_filter": {
    "tags": {
      "exclude": ["internal"]
    },
    "rule_logic": "AND"
  },
  "language_options":{
      "kv_map": {
        "java_package": "com.{name}.{version}.{package}",
        "java_outer_classname": "{module}Proto",
        "java_multiple_files": true,
        "go_package": "github.com/{name}/{package}/{module}pb",
        "csharp_namespace": "MyCompany.{package}.{module}",
        "cc_enable_arenas": true
      }
  },
  "tls": {
    "enabled": true,
    "certificate": "example/guber/certs/localhost.crt",
    "key": "example/guber/certs/localhost.key",
    "ca": "example/guber/certs/root.crt"
  }
}
