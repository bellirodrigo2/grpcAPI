{
  "proto_path": "grpcAPI/proto",
  "lib_path": "grpcAPI/lib",
  "lint": true,
  "host": "localhost",
  "port": 50051,
  "compile_proto": {
    "clean_services": true,
    "overwrite": false
  },
  "service_filter": {
    "package": {
      "include": [],
      "exclude": []
    },
    "module": {
      "include": [],
      "exclude": []
    },
    "tags": {
      "include": [],
      "exclude": []
    },
    "rule_logic": "AND"
  },
  "format": {
    "max_char_per_line": 80,
    "title_case": "none",
    "comment_style": "multiline"
  },
  "server": {
    "compression": "gzip",
    "maximum_concurrent_rpcs": 100,
    "options": []
  },
  "plugins": {
    "health_check": {},
    "reflection": {},
    "server_logger": {
      "level": "INFO",
      "handlers": ["console"],
      "filename": ".",
      "propagate": false
    }
  },
  "app_log_level": "INFO",
  "logger": {
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "short": {
        "format": "[%(asctime)s]: %(message)s"
      },
      "default": {
        "format": "[%(asctime)s] [%(levelname)s] %(name)s: %(message)s"
      },
      "detailed": {
        "format": "[%(asctime)s] [%(levelname)s] %(name)s (%(filename)s:%(lineno)d): %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "formatter": "short"
      },
      "file": {
        "class": "logging.FileHandler",
        "filename": "app.log",
        "mode": "a",
        "level": "INFO",
        "formatter": "default"
      }
    },
    "loggers": {
      "grpcAPI": {
        "handlers": ["console"],
        "level": "ERROR",
        "propagate": false
      }
    },
    "root": {
      "handlers": ["console"],
      "level": "WARNING"
    }
  }
}
